{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "crons": [
        {
            "path": "/api/cron/export-signups",
            "schedule": "0 16 * * 1"
        },
        {
            "path": "/api/cron/archive-races",
            "schedule": "0 0 * * *"
        }
    ]
}